<extension xmlns="http://ns.adobe.com/air/extension/32.0">
    <id>com.assukar.air.android.utils</id>
    <versionNumber>1.0.0</versionNumber>
    <platforms>

        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.assukar.air.android.utils.AndroidUtilsExtension</initializer>
                <finalizer>com.assukar.air.android.utils.AndroidUtilsExtension</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.assukar.air.android.utils.AndroidUtilsExtension</initializer>
                <finalizer>com.assukar.air.android.utils.AndroidUtilsExtension</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment />
        </platform>

    </platforms>
</extension>